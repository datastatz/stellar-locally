{
  "name": "edu-os",
  "version": "0.1.0",
  "main": "electron/main/main.ts",
  "private": true,
  "scripts": {
    "dev:web": "cd . && next dev --turbopack",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "dev": "concurrently -k \"npm run dev:web\" \"npm run dev:electron\"",
    "build:web": "cd . && next build",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint ."
  },
  "dependencies": {
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^9.0.1"
  }
}
