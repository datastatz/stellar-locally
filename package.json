{
  "name": "edu-os",
  "version": "0.1.0",
  "type": "module",
  "main": "electron/main/main.ts",
  "private": true,
  "scripts": {
    "dev:web": "cd . && next dev --turbopack",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "dev": "concurrently -k \"npm run dev:web\" \"npm run dev:electron\"",
    "build:web": "cd . && next build",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint ."
  },
  "dependencies": {
    "@openai/agents": "^0.1.5",
    "@openai/agents-realtime": "^0.1.5",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "pdfjs-dist": "^3.11.174",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "styled-components": "^6.1.19",
    "three": "^0.180.0",
    "three-stdlib": "^2.36.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^9.0.1"
